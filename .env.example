# 应用基础配置
VITE_APP_NAME=Aura Flow
VITE_BUILD_VERSION=1.0.0

# API 配置
VITE_API_BASE_URL=http://localhost:3001
VITE_API_TIMEOUT=10000

# AI 服务配置
VITE_AI_PROVIDER=mock
VITE_AI_API_KEY=
VITE_AI_MODEL=gpt-3.5-turbo

# 可选 AI 提供方配置
# OpenAI
# VITE_AI_PROVIDER=openai
# VITE_AI_API_KEY=sk-your-openai-api-key
# VITE_AI_MODEL=gpt-3.5-turbo

# 通义千问
# VITE_AI_PROVIDER=qwen
# VITE_AI_API_KEY=your-qwen-api-key
# VITE_AI_MODEL=qwen-turbo

# Minimax
# VITE_AI_PROVIDER=minimax
# VITE_AI_API_KEY=your-minimax-api-key
# VITE_AI_MODEL=abab5.5-chat

# 监控和遥测
VITE_SENTRY_DSN=
VITE_ENABLE_TELEMETRY=false

# 数据库配置（后端使用）
DATABASE_URL=file:./dev.db
DATABASE_PROVIDER=sqlite

# 认证配置（后端使用）
JWT_SECRET=your-super-secret-jwt-key-change-in-production
JWT_EXPIRES_IN=7d

# 邮件服务配置（可选）
SMTP_HOST=
SMTP_PORT=587
SMTP_USER=
SMTP_PASS=

# 文件存储配置（可选）
STORAGE_PROVIDER=local
STORAGE_PATH=./uploads

# 开发环境特定配置
NODE_ENV=development

# AI 提供商配置（仅用于显示/切换）
VITE_AI_PROVIDER=openai
